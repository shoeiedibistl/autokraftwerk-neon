{
  "main": "app.js",
  "scripts": {
    "build": "rimraf public/dist && webpack --config webpack/webpack.config.js --env NODE_ENV=production --mode=production --progress",
    "dev": "webpack server --config webpack/webpack.config.js --env NODE_ENV=development --mode development --progress --open",
    "script": "rimraf public/dist && webpack --config webpack/webpack.config.script.js --env NODE_ENV=production --mode=production",
    "deploy": "cd public && git add . && git commit -m 'update' && git push",
    "build + deploy": "npm run build && npm run deploy",
    "set up piblic": "cd public && git init && git remote add origin https://github.com/cloudmill/test-public.git && git add . && git commit -m 'update' && git branch -M main && git push -u origin main"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-typescript": "^7.18.6",
    "autoprefixer": "^10.4.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "beautify-html-webpack-plugin": "^1.0.3",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass-glob-importer": "^5.3.2",
    "pug": "^3.0.2",
    "pug-html-loader": "^1.1.5",
    "pug-plain-loader": "^1.1.0",
    "pug-plugin": "^4.9.7",
    "rimraf": "^3.0.2",
    "sass": "^1.54.2",
    "sass-loader": "^10.3.1",
    "style-loader": "^2.0.0",
    "svg-spritemap-webpack-plugin": "^3.9.1",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
